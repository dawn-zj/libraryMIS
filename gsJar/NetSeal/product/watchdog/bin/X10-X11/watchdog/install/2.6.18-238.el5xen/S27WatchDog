ifinsmod=0
str=`/sbin/modprobe hwmon`
if [ "" != "$str" ]
then
	echo "hwmon not exist"
	insmod /lib/modules/`uname -r`/kernel/drivers/hwmon/hwmon.ko
	ifinsmod=1 
fi
str=`/sbin/modprobe hwmon_vid`
if [ "" != "$str" ]
then
	echo "hwmon_vid not exist"
	insmod /lib/modules/`uname -r`/kernel/drivers/hwmon/hwmon-vid.ko
	ifinsmod=1
fi

insmod /lib/modules/`uname -r`/kernel/drivers/hwmon/w83627ehf.ko
if [ -e /lib/modules/`uname -r`/kernel/drivers/hwmon/w83795.ko ]
then
	insmod /lib/modules/`uname -r`/kernel/drivers/hwmon/w83795.ko
fi

str=`/sbin/modprobe coretemp`
if [ "" != "$str" ]
then
        insmod /lib/modules/`uname -r`/kernel/drivers/hwmon/coretemp.ko
fi

agetty /dev/ttyS1 9600 vt100 &
sleep 3
pid=`ps -ef | grep agetty | grep ttyS1 | awk '{ print $2}'`
kill -9 $pid

export PATH=/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin:/root/bin
cd  /opt/infosec/NetSignServer52/NetSignServer/WatchDog/bin/
./start.sh &
